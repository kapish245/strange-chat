    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.2.0/socket.io.js"></script>
    <title>Stranger chat</title>
</head>

<body>
    <div class="wrapper">
        <div class="chat">
            <div class="chat-header"><span class="naam" id="naam"> <%= nick + " " %></span><span><i class="fa fa-circle fa-sm" aria-hidden="true"></i></span></div>
            <div>
            <div id="output"></div>
            <div id="typing"></div>
          </div>
        </div>
        <div class="input">
            <input type="text" id="message">
            <button id="send">send</button>
        </div>

    </div>
    <script src="js/chat.js">
    </script>
</body>

</html>
